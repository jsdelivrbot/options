page-si-setdp {
    ._tittle {
        font-family: PingFangSC-Medium;
        font-size: 2.67rem;
        color: #000000;
        letter-spacing: 0;
        font-weight: bold;
        padding-bottom: 2rem;
    }
    .sub_tittle {
        font-family: PingFangSC-Regular;
        font-size: 1.26rem;
        color: #000000;
        letter-spacing: 0;
        text-align: center;
    }
    .input_wrap {
        width: 80%;
        margin: 0 auto;
        height: 3.33rem;
        position: relative;
        display: flex;
        ion-input {
            width: 100%;
            height: 100%;
            position: absolute;
            input {
                width: 100%;
                height: 100%;
                margin: 0;
                opacity: 0;
            }
        }
        &>div {
            flex: 1;
            height: 100%;
            background: #FFFFFF;
            border: 1px solid #CCCCCC;
            &>div {
                width: 0.83rem;
                height: 0.83rem;
                margin: 0 auto;
                margin-top: 1.25rem;
                border-radius: 50%;
                background: #333333;
            }
        }
        &>div:nth-child(2) {
            border-radius: 0.4rem 0 0 0.4rem;
        }
        &>div:nth-child(3) {
            border-left: none;
            border-right: none;
        }
        &>div:nth-child(5) {
            border-left: none;
            border-right: none;
        }
        &>div:nth-child(5) {
            border-left: none;
            border-right: none;
        }
        &>div:nth-child(7) {
            border-left: none;
            border-radius: 0 0.4rem 0.4rem 0;
        }
    }
    .tip {
        font-family: PingFangSC-Regular;
        font-size: 1rem;
        color: #CCCCCC;
        letter-spacing: 0;
        text-align: center;
    }
}