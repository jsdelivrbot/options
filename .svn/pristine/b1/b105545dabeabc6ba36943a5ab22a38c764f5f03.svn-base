page-coupon {
    .tip {
        position: relative;
        width: 100%;
        height: 3.33rem;
        &>div {
            position: absolute;
            width: 26.7%;
            height: 1px;
            background: #999999;
            left: 10.7%;
            top: 1.52rem;
        }
        &>div:nth-child(1) {
            left: 62.7%;
        }
        p {
            text-align: center;
            font-family: PingFangSC-Regular;
            font-size: 1rem;
            color: #999999;
            letter-spacing: 2px;
            line-height: 3.33rem;
        }
    }
    .coupon {
        width: 92%;
        height: 7.5rem;
        background: url('../../../../assets/images/my/mycoupon_bg@2x.png') no-repeat;
        background-size: 100% 100%;
        margin: 0.6rem auto;
        position: relative;
        p {
            position: absolute;
            em {
                font-style: normal;
                font-size: 1.5rem;
            }
        }
        .p1 {
            font-family: PingFangSC-Medium;
            font-size: 1.5rem;
            color: #DF3F43;
            letter-spacing: 0;
            left: 4%;
            top: 0.63rem;
            font-weight: 600;
        }
        .p2 {
            font-family: PingFangSC-Regular;
            font-size: 1rem;
            color: #000000;
            letter-spacing: 0;
            left: 4%;
            top: 2.5rem;
            width: 58%;
        }
        .p3 {
            font-family: PingFangSC-Regular;
            font-size: 1rem;
            color: #999999;
            letter-spacing: 0;
            left: 4%;
            top: 5.5rem;
            width: 57%;
            overflow: hidden;
            white-space: nowrap;
        }
        .p4 {
            font-family: PingFangSC-Medium;
            font-size: 2.54rem;
            color: #FFFFFF;
            letter-spacing: 2px;
            left: 75.5%;
            top: 1.5rem;
            font-weight: 600;
        }
        .p5 {
            font-family: PingFangSC-Regular;
            font-size: 1rem;
            color: #FFFFFF;
            letter-spacing: 0;
            text-align: center;
            left: 77.5%;
            bottom: 1.67rem;
        }
    }
}