my-users {
    .list_title {
        position: relative;
        text-align: center;
        height: 3.33rem;
        span {
            font-family: PingFangSC-Regular;
            font-size: 1.25rem;
            color: #999999;
            letter-spacing: 1.67px;
            line-height: 3.33rem;
        }
        span:nth-child(1) {
            position: absolute;
            left: 1.25rem;
        }
        span:nth-child(3) {
            position: absolute;
            right: 1.25rem;
        }
    }
    .list_item {
        @extend .list_title;
    }
    .nt {
        width: 100%;
        height: 0.83rem;
    }
}